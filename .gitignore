#include<stdio.h>
int main()
{
    int i,j,k;
    printf("enter the number:");
    scanf("%d",&k);
    if(k>=2)
    {
      for(i=1;i<=k;i++)
      {
         printf("\n");
        for(j=1;j<=k;j++)
        {
            if(i==1||i==k||j==1||j==k)
            {
                printf ("*");
               
            }
            else
            {
                printf(" ");
            }
        }
      }
    }
        else
        {
            printf("invalid");
        }
          
    
}

